#include"ATM.h"
/*******************************************************************************
 *                      Functions Definitions                                  *
 *******************************************************************************/
void IBAN_Recieve(char *IBAN){
	for(uint8 charIndex=0;charIndex<10;charIndex++){
	IBAN[charIndex]=SPI_sendReceiveByte;
}
}
